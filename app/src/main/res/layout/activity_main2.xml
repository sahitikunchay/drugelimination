<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.acer.drugeliminator.Main2Activity">
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:layout_alignParentStart="true"
        android:textSize="20dp"
        android:hint="Enter Gender"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText3"
        android:textSize="20dp"
        android:hint="Enter Age"
        android:layout_below="@+id/editText2"
        android:layout_alignParentStart="true"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nausea"
        android:id="@+id/nauseacheckBox"
        android:checked="false"
        android:layout_below="@+id/editText3"
        android:layout_alignParentStart="true"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bladder Cancer"
        android:id="@+id/bloodCancerVheckBox"
        android:layout_alignTop="@+id/nauseacheckBox"
        android:layout_toEndOf="@+id/nauseacheckBox"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weight Loss"
        android:id="@+id/weightLossCheckbox"
        android:layout_below="@+id/nauseacheckBox"
        android:layout_alignParentStart="true"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Decreased Appetite"
        android:id="@+id/appetiteCheckBox"
        android:checked="false"
        android:layout_above="@+id/bloodGlucoseDecreasecheckBox"
        android:layout_alignEnd="@+id/dizzyCheckbox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Decrease Blood Glucose"
        android:id="@+id/bloodGlucoseDecreasecheckBox"
        android:layout_below="@+id/weightLossCheckbox"
        android:layout_alignParentStart="true"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vomiting"
        android:id="@+id/vomitingCheckbox"
        android:layout_below="@+id/bloodGlucoseDecreasecheckBox"
        android:layout_alignParentLeft="true"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dyspnoea"
        android:id="@+id/DyspnoeacheckBox"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/vomitingCheckbox"
        android:layout_alignParentLeft="true"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fatigue"
        android:id="@+id/fatiguecheckBox8"
        android:layout_below="@+id/DyspnoeacheckBox"
        android:layout_alignParentLeft="true"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dizziness"
        android:id="@+id/dizzyCheckbox"
        android:layout_alignTop="@+id/bloodGlucoseDecreasecheckBox"
        android:layout_toEndOf="@+id/bloodGlucoseDecreasecheckBox"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diarrhoea"
        android:id="@+id/diarrCheckbox"
        android:checked="false"
        android:layout_below="@+id/bloodGlucoseDecreasecheckBox"
        android:layout_alignStart="@+id/cswellingheckBox11"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Swelling and Cramps"
        android:id="@+id/cswellingheckBox11"
        android:layout_below="@+id/diarrCheckbox"
        android:layout_toEndOf="@+id/vomitingCheckbox"
        android:checked="false"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pain"
        android:id="@+id/PaincheckBox12"
        android:checked="false"
        android:layout_below="@+id/DyspnoeacheckBox"
        android:layout_toEndOf="@+id/vomitingCheckbox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bllod Glucose Increase"
        android:id="@+id/bgincreasecheckBox13"
        android:layout_below="@+id/fatiguecheckBox8"
        android:layout_alignParentStart="true"
        android:checked="false"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:id="@+id/button2"
        android:layout_below="@+id/AbdominalDomfortcheckBox"
        android:layout_toEndOf="@+id/PaincheckBox12"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Headache"
        android:id="@+id/headachecheckBox"
        android:checked="false"
        android:layout_below="@+id/editText3"
        android:layout_toEndOf="@+id/bloodCancerVheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Erucation"
        android:id="@+id/ErucationcheckBox"
        android:checked="false"
        android:layout_below="@+id/bloodGlucoseDecreasecheckBox"
        android:layout_toEndOf="@+id/bloodGlucoseDecreasecheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Somnolence"
        android:id="@+id/SomnolencecheckBox"
        android:checked="false"
        android:layout_above="@+id/bgincreasecheckBox13"
        android:layout_alignStart="@+id/button2"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dyspepsia"
        android:id="@+id/DyspepsiacheckBox"
        android:checked="false"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/SomnolencecheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Discomfort"
        android:id="@+id/DiscomfortcheckBox"
        android:checked="false"
        android:layout_below="@+id/bgincreasecheckBox13"
        android:layout_alignParentStart="true"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Oropharengial Pain"
        android:id="@+id/OropharengialcheckBox"
        android:checked="false"
        android:layout_below="@+id/bgincreasecheckBox13"
        android:layout_alignEnd="@+id/SomnolencecheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Extreme Pain"
        android:id="@+id/ExtremecheckBox"
        android:checked="false"
        android:layout_below="@+id/SomnolencecheckBox"
        android:layout_alignStart="@+id/ErucationcheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ineffectiveness"
        android:id="@+id/ineffectivecheckBox"
        android:checked="false"
        android:layout_below="@+id/DiscomfortcheckBox"
        android:layout_alignParentStart="true"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cold Flashes"
        android:id="@+id/coldcheckBox2"
        android:checked="false"
        android:layout_below="@+id/OropharengialcheckBox"
        android:layout_toEndOf="@+id/ineffectivecheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Arethralgia"
        android:id="@+id/ArethralgiacheckBox"
        android:checked="false"
        android:layout_below="@+id/OropharengialcheckBox"
        android:layout_toEndOf="@+id/button2"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Flatulence"
        android:id="@+id/FlatulencecheckBox"
        android:checked="false"
        android:layout_below="@+id/ineffectivecheckBox"
        android:layout_alignParentStart="true"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Abdominal Distention"
        android:id="@+id/AbdominalDistentioncheckBox"
        android:checked="false"
        android:layout_below="@+id/ineffectivecheckBox"
        android:layout_toEndOf="@+id/DiscomfortcheckBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Abdominal Discomfort"
        android:id="@+id/AbdominalDomfortcheckBox"
        android:checked="false"
        android:layout_below="@+id/FlatulencecheckBox"
        android:layout_alignParentStart="true"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Constipation"
        android:id="@+id/ConstipationcheckBox"
        android:checked="false"
        android:layout_alignBottom="@+id/AbdominalDomfortcheckBox"
        android:layout_toEndOf="@+id/diarrCheckbox"/>

</RelativeLayout>
